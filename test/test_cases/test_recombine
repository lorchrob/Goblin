<SAE_PACKET> ::= <COMMIT> ;
<COMMIT> ::= <AUTH_ALGO> <REJECTED_GROUPS>
{<AUTH_ALGO> <- int_to_bv(16, 3);};

<AUTH_ALGO> :: BitVec(16);

<REJECTED_GROUPS> ::= <RG_ID_LENGTH>  <RG_ID_LIST> 
{ <RG_ID_LENGTH> <- int_to_bv(8, length(<RG_ID_LIST>)); };

<RG_ID_LENGTH>   :: BitVec(8); 

<RG_ID_LIST> ::= <RG_ID> | <RG_ID> <RG_ID_LIST>;

<RG_ID> :: BitVec(8);
