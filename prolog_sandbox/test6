<S> ::= <A> <A> <B> <F> <F> <D> <D> 
{ <A>.<E>.<C> > <B>; <A>.<H>.<C> > 0; <F>.<E>.<C> < <B>; <D> > 0; 
  <A>.<E>.<X>.<Y> > 0; };
<F> ::= <E> <E> | <E> <B>;
<A> ::= <E> <E> <H>;
<H> ::= <C>;
<E> ::= <C> <D> | <C> <C> <C> | <D> | <X> ;
<X> ::= <Y>;
<Y> :: Int ;
<B> :: Int ;
<C> :: Int ;
<D> :: Int { <D> > 0; } ;
